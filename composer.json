{
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "^3.2",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/migrations": "~1.0",
        "cakephp/plugin-installer": "*",
        "qobo/phake-builder": "^1.0",
        "cakedc/users": "~3.1",
        "friendsofcake/bootstrap-ui": "~0.3",
        "burzum/cakephp-file-storage": "^1.1",
        "burzum/cakephp-imagine-plugin": "^2.1",
        "qobo/cakephp-groups": "^1.0",
        "qobo/cakephp-qobo-admin-panel": "^1.0",
        "qobo/cakephp-menu": "^1.0",
        "qobo/cakephp-roles-capabilities": "^1.0",
        "psy/psysh": "^0.7.2",
        "symfony/var-dumper": "^3.0",
        "nikic/php-parser": "^2.0",
        "sebastian/environment": "^1.3",
        "cakephp/debug_kit": "^3.2",
        "muffin/oauth2": "^1.0@dev",
        "cakephp/chronos": "^0.4.8",
        "cakephp/bake": "^1.2"

    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "phpunit/phpunit-selenium": "~2.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "ext-xdebug": "Allows code coverage reports and advanced debugging",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
